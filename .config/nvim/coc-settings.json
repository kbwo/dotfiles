{
  "[go]": {
    "coc.preferences.formatOnSave": true
  },
  "[python]": {
    "coc.preferences.formatOnSave": true
  },
  "[javascript]": {
    "coc.preferences.formatOnSave": true
  },
  "[typescript]": {
    "coc.preferences.formatOnSave": true
  },
  "[typescriptreact]": {
    "coc.preferences.formatOnSave": true
  },
  "[rust]": {
    "coc.preferences.formatOnSave": true
  },
  "[json]": {
    "coc.preferences.formatOnSave": true
  },
  "[jsonc]": {
    "coc.preferences.formatOnSave": true
  },
  "[html]": {
    "coc.preferences.formatOnSave": true
  },
  "[css]": {
    "coc.preferences.formatOnSave": true
  },
  "[scss]": {
    "coc.preferences.formatOnSave": true
  },
  "[prisma]": {
    "coc.preferences.formatOnSave": true
  },
  "[elm]": {
    "coc.preferences.formatOnSave": true
  },
  "[vue]": {
    "coc.preferences.formatOnSave": true
  },
  "[dart]": {
    "coc.preferences.formatOnSave": true
  },
  "rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "translator.toLang": "ja",
  "translator.engines": ["google"],
  "cssmodules.hintName": "cssmodules",
  "prettier.onlyUseLocalVersion": true,
  "python.pythonPath": "~/.pyenv/shims/python",
  "python.jediEnabled": false,
  "coc.preferences.colorSupport": true,
  "coc.source.file.ignoreHidden": true,
  "deno.enable": false,
  "flutter.lsp.initialization.closingLabels": true,
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "languageserver": {
    "kotlin": {
      "command": "~/lsp/kotlin/server/bin/kotlin-language-server",
      "filetypes": [
        "kotlin"
      ]
    },
    "elmLS": {
      "command": "elm-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ],
      "initializationOptions": {
        "elmPath": "./node_modules/.bin/elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "diagnosticsOnSaveOnly": true
      }
    },
    "sql": {
      "command": "sqls",
      "args": [
        "-config",
        "$HOME/.config/sqls/config.yml"
      ],
      "filetypes": [
        "sql"
      ],
      "shell": true
    },
    "terraform": {
      "command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
    }
  },
  "svelte.enable-ts-plugin": true,
  "tabnine.priority": 50,
  "suggest.noselect": true,
  "tabnine.enablePreselect": false,
  "suggest.enablePreselect": false
}
