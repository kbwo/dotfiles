{
  "[javascript][typescript][typescriptreact][json][jsonc][html][css][scss][html]": {
    "coc.preferences.formatOnSave": true,
    "coc.preferences.formatterExtension": "coc-prettier"
  },
  "[go][python][rust][prisma][elm][vue][dart]": {
    "coc.preferences.formatOnSave": true,
  },
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.diagnostics.disabled": [
    "unresolved-proc-macro",
    "inactive-code"
  ],
  // "rust-analyzer.cargo.features": "all",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.inlayHints.bindingModeHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "inlayHint.enable": false,
  // "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  // "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
  // "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  // "typescript.inlayHints.variableTypes.enabled": true,
  // "typescript.inlayHints.parameterNames.enabled": "all",
  // "typescript.inlayHints.parameterTypes.enabled": true,
  // "typescript.inlayHints.enumMemberValues.enabled": true,
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "translator.toLang": "ja",
  "translator.engines": ["google"],
  "cssmodules.hintName": "cssmodules",
  "css.validate": false,
  "css.completion.completePropertyWithSemicolon": false,
  "coc.source.file.ignoreHidden": true,
  "flutter.lsp.initialization.closingLabels": true,
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "languageserver": {
    "coffee": {
      "command": "coffeesense-language-server",
      "filetypes": ["coffee"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "kotlin": {
      "command": "~/go/src/github.com/fwcd/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "args": ["--stdio"],
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "./node_modules/.bin/elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "diagnosticsOnSaveOnly": true
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "notification.disabledProgressSources": [],
  "notification.statusLineProgress": false,
  "svelte.enable-ts-plugin": true,
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "Lua.workspace.library": [
    "/usr/share/nvim/runtime/lua",
    "/usr/share/nvim/runtime/lua/vim",
    "/usr/share/nvim/runtime/lua/vim/lsp",
    "~/.vim/plugged"
  ],
  "Lua.diagnostics.globals": ["vim"],
  "Lua.workspace.checkThirdParty": false,
  "yaml.customTags": [
    "!And",
    "!Base64",
    "!Cidr",
    "!Equals",
    "!FindInMap scalar",
    "!FindInMap sequence",
    "!GetAtt",
    "!GetAZs",
    "!If",
    "!ImportValue",
    "!Join",
    "!Not",
    "!Or",
    "!Ref scalar",
    "!Select",
    "!Split",
    "!Sub scalar",
    "!Sub sequence",
    "!And sequence",
    "!If sequence",
    "!Not sequence",
    "!Equals sequence",
    "!Or sequence",
    "!FindInMap",
    "!Join sequence",
    "!Ref",
    "!Sub",
    "!ImportValue sequence",
    "!Select sequence",
    "!Split sequence"
  ],
  "java.progressReports.enabled": false,
  "rust-analyzer.check.overrideCommand": [
    "cargo",
    "check",
    "--all-targets",
    "--workspace",
    "--all-features",
    "--message-format=json",
    "--jobs",
    "4"
  ],
  "intelephense.format.braces": "per",
  "workspace.rootPatterns": [".git", ".hg", ".projections.json", ".vim"],
  "tsserver.log": "verbose",
  "tree.key.close": "x",
  "tree.key.activeFilter": "i",
  "outline.expandLevel": 10,
  "outline.autoWidth": true,
  "tree.key.toggleSelection": "m",
  "tree.key.collapseAll": "O",
  "outline.autoPreview": false,
  "outline.sortBy": "position",
  "yaml.schemas": {
    "espanso": [
      "https://raw.githubusercontent.com/espanso/espanso/dev/schemas/match.schema.json"
    ]
  }
}
