{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "python",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "html",
    "css",
    "scss",
    "prisma",
    "dart"
  ],
  "cssmodules.hintName": "cssmodules",
  "prettier.onlyUseLocalVersion": true,
  "python.pythonPath": "~/.pyenv/shims/python",
  "python.jediEnabled": false,
  "coc.preferences.colorSupport": true,
  "coc.source.file.ignoreHidden": true,
  "deno.enable": false,
  "html-css-support.enabledLanguages": [
    "html",
    "vue",
    "blade",
    "htmldjango",
    "typescriptreact",
    "javascriptreact"
  ],
  "languageserver": {
    "kotlin": {
      "command": "~/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": [
        "kotlin"
      ]
    },
    "sql": {
      "command": "sqls",
      "args": [
        "-config",
        "$HOME/.config/sqls/config.yml"
      ],
      "filetypes": [
        "sql"
      ],
      "shell": true
    }
  }
}
